{
  "port": 3000,
  "storage": "mongoDB",
  "currentContainersFile": ".attachedContainers.json",
  "logErrorsOfAll": true,
  "logStoppedContainersAlso": true,
  "imageSettings": {
    "exampleimage": {
      "lowListenThreshold": 5,
      "attachOneForEvery" : 2,
      "maxAttached": 10
    },
    "*": {
      "lowListenThreshold": 2,
      "attachOneForEvery" : 2,
      "maxAttached": 10
    }
  }
}
