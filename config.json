{
  "port": 3000,
  "storage": "mongoDB",
  "currentContainersFile": ".attachedContainers.json",
  "logErrorsOfAll": true,
  "logStoppedContainersAlso": false,
  "imageSettings": {
    "exampleimage": {
      "lowListenThreshold": 5,
      "attachOneForEvery" : 3,
      "maxAttached": 80
    },
    "examplepercent" : {
      "lowListenThreshold": 1,
      "attachOneForEvery" : 10,
      "maxAttached": 50      
    },
    "*": {
      "lowListenThreshold": 2,
      "attachOneForEvery" : 1,
      "maxAttached": 10
    }
  }
}
